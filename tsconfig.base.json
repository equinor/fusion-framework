{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "incremental": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "stripInternal": true,
    "skipLibCheck": true,
    "lib": ["esnext", "dom", "dom.iterable"],
    "composite": true
  }
}
