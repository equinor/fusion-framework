{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "dist/esm",
    "rootDir": "src",
    "declarationDir": "./dist/types",
    "baseUrl": "src",
    "paths": {
      "@equinor/fusion-framework-module-services": [
        "."
      ],
      "@equinor/fusion-framework-module-services/*": [
        "./*"
      ]
    }
  },
  "references": [
    {
      "path": "../observable"
    },
    {
      "path": "../module"
    },
    {
      "path": "../module-http"
    },
    {
      "path": "../module-service-discovery"
    }
  ],
  "include": [
    "src/**/*",
  ],
  "exclude": [
    "node_modules",
    "lib"
  ]
}