{
  "name": "@equinor/fusion-framework-dev-server",
  "version": "0.0.0",
  "description": "Package for running a development server for fusion-framework",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./plugin-api-service": {
      "import": "./dist/esm/plugin/api-service/index.js",
      "types": "./dist/types/plugin/api-service/index.d.ts"
    }
  },
  "types": "dist/types/index.d.ts",
  "typesVersions": {
    "*": {
      "plugin-api-service": [
        "dist/types/plugin/api-service/index.d.ts"
      ]
    }
  },
  "directories": {
    "dist": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/equinor/fusion-framework.git",
    "directory": "packages/dev-server"
  },
  "scripts": {
    "build": "tsc -b",
    "test": "vitest"
  },
  "keywords": [
    "dev-server",
    "fusion-framework",
    "vite",
    "typescript"
  ],
  "license": "ISC",
  "dependencies": {
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^3.0.3",
    "path-to-regexp": "^8.2.0"
  },
  "devDependencies": {
    "@types/http-proxy": "^1.17.16",
    "nock": "^14.0.1",
    "typescript": "^5.5.4",
    "vite": "^6.0.7",
    "vitest": "^2.0.5"
  }
}