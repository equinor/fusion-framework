{
  "name": "@equinor/fusion-imports",
  "version": "1.1.2",
  "description": "Package import files and configurations",
  "keywords": [
    "esbuild",
    "typescript",
    "transpiling",
    "node",
    "runtime"
  ],
  "license": "ISC",
  "type": "module",
  "main": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
