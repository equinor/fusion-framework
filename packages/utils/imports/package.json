  },
  "bugs": {
    "url": "https://github.com/equinor/fusion-framework/issues"
  },
  "dependencies": {
    "esbuild": "^0.25.1",
    "read-package-up": "^11.0.0"
  },
  "devDependencies": {
    "fs-monkey": "^1.1.0",
    "memfs": "^4.17.0",
    "typescript": "^5.8.2",
    "unionfs": "^4.5.4",
    "vitest": "^3.2.4"
  }
