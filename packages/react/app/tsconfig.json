    "outDir": "dist/esm",
    "rootDir": "src",
    "declarationDir": "./dist/types",
    "paths": {
      "react": ["./node_modules/@types/react"]
    }
  },
  "references": [
    {
      "path": "../../utils/observable"
    },
    {
      "path": "../../app"
    },
    {
      "path": "../framework"
    },
    {
      "path": "../../modules/ag-grid"
    },
    {
      "path": "../../modules/navigation"
    },
    {
      "path": "../modules/module"
    },
    {
      "path": "../modules/http"
    },
    {
      "path": "../modules/context"
    },
    {
      "path": "../modules/bookmark"
    },
    {
      "path": "../widget"
    }
  ],
  "include": ["src/**/*"],
  "exclude": ["node_modules", "lib"]
}
