{
  "name": "@equinor/fusion-framework-cli-plugin-ai-base",
  "version": "1.0.0",
  "description": "Base AI plugin package for Fusion Framework CLI",
  "type": "module",
  "main": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./command-options": {
      "import": "./dist/esm/options/index.js",
      "types": "./dist/types/options/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "dist/types/index.d.ts"
      ],
      "./command-options": [
        "dist/types/options/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc -b",
    "build:types": "tsc -b",
    "prepack": "pnpm build",
    "test": "vitest"
  },
  "keywords": [
    "fusion-framework",
    "cli",
    "plugin",
    "llm",
    "ai"
  ],
  "author": "",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/equinor/fusion-framework.git",
    "directory": "packages/cli-plugins/ai-base"
  },
  "dependencies": {
    "@equinor/fusion-framework-module-ai": "workspace:*",
    "@equinor/fusion-framework-module": "workspace:*",
    "@equinor/fusion-imports": "workspace:*",
    "commander": "^14.0.1",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "@equinor/fusion-framework-cli": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.8.2",
    "vitest": "^3.2.4"
  }
}
