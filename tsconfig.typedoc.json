{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "noEmit": false,
    "removeComments": false,
    "declaration": true,
    "skipLibCheck": true,
    "jsx": "react-jsx"
  },
  "typedocOptions": {
    "entryPoints": [
      "./packages/framework",
      "./packages/framework-react",
      "./packages/module",
      "./packages/module-http",
      "./packages/module-msal",
      "./packages/module-msal/client",
      "./packages/module-service-discovery",
      "./packages/react-app",
      "./packages/react-app/hooks",
      // "./packages/observable",
      // "./packages/observable/operators",
      // "./packages/observable/query",
      // "./packages/observable/react",
    ],
    "entryPointStrategy": "packages",
    "readme": "./README.md",
    "out": "docs",
    "hideGenerator": true,
    "excludeExternals": true,
    "exclude": [
      "**/__test__/**/*",
      "**/dist/**/*",
      "**/node_modules/**/*",
    ],
    "includes": [
      "./packages",
    ],
  }
}